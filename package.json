{
  "name": "joiful-react-forms",
  "version": "0.1.3",
  "description": "Automatically generate validated React forms using Joi",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": ">=0.13.0"
  },
  "keywords": [
    "react",
    "joi",
    "react-component",
    "form"
  ],
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-decorators-legacy"
    ],
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ]
  },
  "scripts": {
    "lint": "eslint src/**",
    "prebuild": "rm -rf lib && rm -rf build",
    "build": "babel -d lib/ src/",
    "prepublish": "npm run lint && npm run build",
    "test": "npm run lint; mocha spec --compilers js:babel-register"
  },
  "devDependencies": {
    "autobind-decorator": "^1.3.3",
    "babel-cli": "^6.2.0",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-jsx-control-statements": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "expect": "^1.20.1",
    "lodash": "^4.0.0",
    "mocha": "^2.5.3",
    "react": "^0.14.6",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^0.14.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjanoudi/joiful-react-forms"
  },
  "author": "Kareem Janoudi",
  "license": "MIT",
  "dependencies": {
    "joi": "^8.0.5",
    "react-tap-event-plugin": "^0.2.2"
  }
}
